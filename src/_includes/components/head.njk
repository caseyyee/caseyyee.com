<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{title}}</title>

  <script>
    (function () {
      if (location.protocol === 'http:' && location.hostname === 'caseyyee.com') {
        location.href = 'https:' + location.href.substring(location.protocol.length);
      }
    })();
  </script>
  <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,600" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css2?family=Lora:wght@600&display=swap" rel="stylesheet">

  {%- set css %}
  {% include 'index.css' %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>

  <!-- Twitter card -->
  <meta name="twitter:card" content="summary">
  <meta name="twitter:site" content="caseyyee.com">
  <meta name="twitter:title" content="{{ title }}">
  <meta name="twitter:creator" content="@whoyee">
  <meta name="twitter:description" content="{{ description }}">
  <meta name="twitter:image" content="https://caseyyee.com/img/casey.jpg">

  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-136874409-1"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'UA-136874409-1');
  </script>
</head>
